{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ETLDWH1-3"
		},
		"BankMarketBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BankMarketBlob'"
		},
		"ConnectorAzureRafael_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ConnectorAzureRafael'"
		},
		"bankMarketDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'bankMarketDatabase'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ETLDWH1-3_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"BankMarketBlob_connectionString": {
						"value": "[parameters('BankMarketBlob_connectionString')]"
					},
					"ConnectorAzureRafael_connectionString": {
						"value": "[parameters('ConnectorAzureRafael_connectionString')]"
					},
					"bankMarketDatabase_connectionString": {
						"value": "[parameters('bankMarketDatabase_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ETLDWH1-3_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ETLDWH1-3_ArmTemplate_0')]"
			]
		}
	]
}